{
  "expo": {
    "name": "TribalMingle",
    "slug": "tmapp",
    "scheme": "tmapp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0520"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tribalmingle.app",
      "buildNumber": "2",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "backgroundColor": "#0F0520"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.tribalmingle.app",
      "versionCode": 3
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#FF5C00"
        }
      ],
      "expo-secure-store",
      "@react-native-community/datetimepicker"
    ],
    "extra": {
      "router": {
        "origin": "https://tmapp.local"
      },
      "eas": {
        "projectId": "309dc50f-fb46-49f7-bd62-6a1a86d72910"
      }
    },
    "runtimeVersion": "1.0.1",
    "updates": {
      "url": "https://u.expo.dev/309dc50f-fb46-49f7-bd62-6a1a86d72910"
    }
  }
}
