{
  "expo": {
    "name": "TribalMingle",
    "slug": "tmapp",
    "scheme": "tmapp",
    "version": "1.0.3",
    "runtimeVersion": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0F0520"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tribalmingle.app",
      "buildNumber": "3",
      "entitlements": {
        "aps-environment": "production"
      },
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "UIBackgroundModes": [
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0F0520"
      },
      "minSdkVersion": 21,
      "supportedAbis": [
        "armeabi-v7a",
        "arm64-v8a",
        "x86",
        "x86_64"
      ],
      "edgeToEdgeEnabled": true,
      "package": "com.tribalmingle.app",
      "versionCode": 3,
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "CAMERA",
        "INTERNET",
        "MODIFY_AUDIO_SETTINGS",
        "READ_EXTERNAL_STORAGE",
        "RECORD_AUDIO",
        "SYSTEM_ALERT_WINDOW",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "VIBRATE",
        "WRITE_EXTERNAL_STORAGE",
        "POST_NOTIFICATIONS"
      ]
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/logo.png",
          "color": "#FF5C00"
        }
      ],
      "expo-secure-store",
      "@react-native-community/datetimepicker",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": "https://tmapp.local"
      },
      "eas": {
        "projectId": "309dc50f-fb46-49f7-bd62-6a1a86d72910"
      }
    },
    "owner": "ccmendel"
  }
}
